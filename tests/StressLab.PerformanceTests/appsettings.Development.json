{
  "TestConfiguration": {
    "Name": "API Performance Test - Development",
    "Description": "Development environment API performance test",
    "TestType": "Api",
    "DurationSeconds": 30,
    "ConcurrentUsers": 5,
    "RampUpSeconds": 5,
    "ApiEndpoint": "https://httpbin.org/get",
    "ApiMethod": "GET",
    "ExpectedResponseTimeMs": 500,
    "MaxErrorRatePercent": 2.0
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "StressLab": "Trace"
    }
  },
  "Serilog": {
    "MinimumLevel": "Debug"
  }
}

